{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    }
  },
  "include": [
    "*",
    "controllers/server.ts",
    "server.tss",
    "src/controll",
    "src/server.ts",
    "src/planets.ts",
    "src/server.ts"
  ],
  "exclude": ["*.json", "dist"]
}
